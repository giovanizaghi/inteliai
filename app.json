{
  "expo": {
    "name": "inteliai",
    "slug": "inteliai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.giovanizaghi.inteliai",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permita que o aplicativo acesse a biblioteca de fotos para salvar imagens."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.giovanizaghi.inteliai"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
  }
}
